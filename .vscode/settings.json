{
  "editor.tabSize": 2,
  "files.associations": {
      "*.tpl.js": "html",
      "*.css.js": "html",
      "*.tpl.ts": "html",
      "*.css.ts": "html"
  },
  "javascript.implicitProjectConfig.checkJs": true,
  "search.exclude": {
    "node_modules": true,
    "dist": true
  },
  "files.watcherExclude": {
    ".git/objects/**": true,
    ".git/subtree-cache/**": true,
  },
}